{
    "name": "OWASP Juice Shop",
    "image": "juice-shop",
    "build": {
      "context": ".",
      "dockerfile": "Dockerfile"
    },
    "settings": { 
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
      "dbaeumer.vscode-eslint",
      "esbenp.prettier-vscode",
      "angular.ng-template",
      "eg2.vscode-npm-script"
    ],
    "forwardPorts": [3000],
    "postCreateCommand": "npm install",
    "remoteUser": "node"
  }
  